{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug Test",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/push_swap",
          "args": [
              "10 3 2 1 5 78 1"
          ],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
          "preLaunchTask": "build",
          "miDebuggerPath": "/usr/bin/gdb",
          "logging": {
              "engineLogging": true
          }
      }
  ]
}